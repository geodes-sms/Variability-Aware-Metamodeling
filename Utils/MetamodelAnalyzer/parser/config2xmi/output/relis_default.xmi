<?xml version="1.0" encoding="ASCII"?>
<FM:RootFeature xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:FM="http://www.example.org/FM" name="ReLiS" abstract="true">
  <association xsi:type="FM:OptionalAssociation" childfeature="Screening" parentfeature="ReLiS"/>
  <association xsi:type="FM:OptionalAssociation" childfeature="QA" parentfeature="ReLiS"/>
  <association xsi:type="FM:OptionalAssociation" childfeature="DataExtraction" parentfeature="ReLiS"/>
  <association xsi:type="FM:OptionalAssociation" childfeature="Synthesis" parentfeature="ReLiS"/>
  <feature name="Screening" abstract="true">
    <association xsi:type="FM:MandatoryAssociation" childfeature="Criteria" parentfeature="Screening"/>
    <association xsi:type="FM:OptionalAssociation" childfeature="MultipleReviewers" parentfeature="Screening"/>
    <association xsi:type="FM:OptionalAssociation" childfeature="Validation" parentfeature="Screening"/>
    <association xsi:type="FM:MandatoryAssociation" childfeature="Phase" parentfeature="Screening"/>
    <subfeatures name="Criteria" abstract="true">
      <association xsi:type="FM:OptionalAssociation" childfeature="Inclusion" parentfeature="Criteria"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="Exclusion" parentfeature="Criteria"/>
      <subfeatures name="Inclusion"/>
      <subfeatures name="Exclusion"/>
    </subfeatures>
    <subfeatures name="MultipleReviewers" abstract="true">
      <association xsi:type="FM:MandatoryAssociation" childfeature="Conflict" parentfeature="MultipleReviewers"/>
      <subfeatures name="Conflict" abstract="true">
        <association xsi:type="FM:OptionalAssociation" childfeature="ConflictDetection" parentfeature="Conflict"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="ConflictResolution" parentfeature="Conflict"/>
        <subfeatures name="ConflictDetection" abstract="true">
          <association xsi:type="FM:OptionalAssociation" childfeature="IncludeExclude" parentfeature="ConflictDetection"/>
          <subfeatures name="IncludeExclude"/>
        </subfeatures>
        <subfeatures name="ConflictResolution" abstract="true">
          <association xsi:type="FM:OptionalAssociation" childfeature="Majority" parentfeature="ConflictResolution"/>
          <subfeatures name="Majority"/>
        </subfeatures>
      </subfeatures>
    </subfeatures>
    <subfeatures name="Validation" abstract="true">
      <association xsi:type="FM:MandatoryAssociation" childfeature="AssignmentMode" parentfeature="Validation"/>
      <subfeatures name="AssignmentMode" abstract="true">
        <association xsi:type="FM:OptionalAssociation" childfeature="Veto" parentfeature="AssignmentMode"/>
        <subfeatures name="Veto"/>
      </subfeatures>
    </subfeatures>
    <subfeatures name="Phase" abstract="true">
      <association xsi:type="FM:MandatoryAssociation" childfeature="Field" parentfeature="Phase"/>
      <subfeatures name="Field"/>
    </subfeatures>
  </feature>
  <feature name="QA" abstract="true">
    <association xsi:type="FM:MandatoryAssociation" childfeature="Question" parentfeature="QA"/>
    <association xsi:type="FM:MandatoryAssociation" childfeature="Answer" parentfeature="QA"/>
    <subfeatures name="Question"/>
    <subfeatures name="Answer"/>
  </feature>
  <feature name="DataExtraction" abstract="true">
    <association xsi:type="FM:OptionalAssociation" childfeature="Category" parentfeature="DataExtraction"/>
    <subfeatures name="Category" abstract="true">
      <association xsi:type="FM:MandatoryAssociation" childfeature="CategoryType" parentfeature="Category"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="Subcategroies" parentfeature="Category"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="MandatoryCategory" parentfeature="Category"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="Multivalued" parentfeature="Category"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="InitialValue" parentfeature="Category"/>
      <subfeatures name="CategoryType" abstract="true">
        <association xsi:type="FM:OptionalAssociation" childfeature="FreeCategory" parentfeature="CategoryType"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="Note" parentfeature="CategoryType"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="StaticCategory" parentfeature="CategoryType"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="IndependentDynamicCategory" parentfeature="CategoryType"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="DependentDynamicCategory" parentfeature="CategoryType"/>
        <subfeatures name="FreeCategory" abstract="true">
          <association xsi:type="FM:OptionalAssociation" childfeature="String" parentfeature="FreeCategory"/>
          <subfeatures name="String"/>
        </subfeatures>
        <subfeatures name="Note"/>
        <subfeatures name="StaticCategory"/>
        <subfeatures name="IndependentDynamicCategory"/>
        <subfeatures name="DependentDynamicCategory"/>
      </subfeatures>
      <subfeatures name="Subcategroies"/>
      <subfeatures name="MandatoryCategory"/>
      <subfeatures name="Multivalued"/>
      <subfeatures name="InitialValue"/>
    </subfeatures>
  </feature>
  <feature name="Synthesis" abstract="true">
    <association xsi:type="FM:OptionalAssociation" childfeature="CompareGraph" parentfeature="Synthesis"/>
    <association xsi:type="FM:OptionalAssociation" childfeature="Type" parentfeature="Synthesis"/>
    <subfeatures name="CompareGraph"/>
    <subfeatures name="Type" abstract="true">
      <association xsi:type="FM:OptionalAssociation" childfeature="Line" parentfeature="Type"/>
      <subfeatures name="Line"/>
    </subfeatures>
  </feature>
</FM:RootFeature>
