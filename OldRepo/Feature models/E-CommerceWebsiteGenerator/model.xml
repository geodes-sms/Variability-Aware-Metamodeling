<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="autolayoutconstraints" value="false"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showconstraints" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and mandatory="true" name="ECommerce">
			<graphics key="collapsed" value="false"/>
			<and mandatory="true" name="ItemCatalog">
				<or abstract="true" mandatory="true" name="Item">
					<feature name="Service"/>
					<feature name="Product"/>
				</or>
			</and>
			<and mandatory="true" name="ShoppingCart">
				<feature name="Discount"/>
			</and>
			<alt abstract="true" mandatory="true" name="PaymentType">
				<feature name="MultiplePayments"/>
				<feature name="SinglePayment"/>
			</alt>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>Product</var>
				<var>Discount</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Service</var>
				<not>
					<var>MultiplePayments</var>
				</not>
			</imp>
		</rule>
	</constraints>
</featureModel>
