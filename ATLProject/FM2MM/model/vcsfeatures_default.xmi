<?xml version="1.0" encoding="ASCII"?>
<FM:RootFeature xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:FM="http://www.example.org/FM" name="VCSFeatures">
  <association xsi:type="FM:MandatoryAssociation" childfeature="Commands" parentfeature="VCSFeatures"/>
  <association xsi:type="FM:MandatoryAssociation" childfeature="History" parentfeature="VCSFeatures"/>
  <association xsi:type="FM:MandatoryAssociation" childfeature="CollaborationStyle" parentfeature="VCSFeatures"/>
  <association xsi:type="FM:MandatoryAssociation" childfeature="Database" parentfeature="VCSFeatures"/>
  <association xsi:type="FM:OptionalAssociation" childfeature="VersionUnit" parentfeature="VCSFeatures"/>
  <feature name="Commands">
    <association xsi:type="FM:OptionalAssociation" childfeature="HighLevelCommand" parentfeature="Commands"/>
    <association xsi:type="FM:MandatoryAssociation" childfeature="LowLevelCommand" parentfeature="Commands"/>
    <association xsi:type="FM:OptionalAssociation" childfeature="MergeStrategy" parentfeature="Commands"/>
    <subfeatures name="HighLevelCommand"/>
    <subfeatures name="LowLevelCommand">
      <association xsi:type="FM:MandatoryAssociation" childfeature="LowCommands" parentfeature="LowLevelCommand"/>
      <subfeatures name="LowCommands" abstract="true">
        <association xsi:type="FM:MandatoryAssociation" childfeature="Add" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="Checkout" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="CheckoutCreate" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="Clone" parentfeature="LowCommands"/>
        <association xsi:type="FM:MandatoryAssociation" childfeature="Commit" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="CherryPick" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="CreateBranch" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="Fetch" parentfeature="LowCommands"/>
        <association xsi:type="FM:MandatoryAssociation" childfeature="Init" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="Log" parentfeature="LowCommands"/>
        <association xsi:type="FM:MandatoryAssociation" childfeature="Open" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="Pull" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="Push" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="RemoteAdd" parentfeature="LowCommands"/>
        <association xsi:type="FM:OptionalAssociation" childfeature="Rm" parentfeature="LowCommands"/>
        <subfeatures name="Add"/>
        <subfeatures name="Checkout"/>
        <subfeatures name="CheckoutCreate"/>
        <subfeatures name="Clone"/>
        <subfeatures name="Commit"/>
        <subfeatures name="CherryPick"/>
        <subfeatures name="CreateBranch"/>
        <subfeatures name="Fetch"/>
        <subfeatures name="Init"/>
        <subfeatures name="Log"/>
        <subfeatures name="Open"/>
        <subfeatures name="Pull"/>
        <subfeatures name="Push"/>
        <subfeatures name="RemoteAdd"/>
        <subfeatures name="Rm"/>
      </subfeatures>
    </subfeatures>
    <subfeatures name="MergeStrategy" abstract="true">
      <association xsi:type="FM:OptionalAssociation" childfeature="DSMCompare" parentfeature="MergeStrategy"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="GitDiff" parentfeature="MergeStrategy"/>
      <subfeatures name="DSMCompare"/>
      <subfeatures name="GitDiff"/>
    </subfeatures>
  </feature>
  <feature name="History">
    <association xsi:type="FM:OptionalAssociation" childfeature="BranchingStyle" parentfeature="History"/>
    <subfeatures name="BranchingStyle" abstract="true">
      <association xsi:type="FM:OptionalAssociation" childfeature="MultiBranch" parentfeature="BranchingStyle"/>
      <subfeatures name="MultiBranch" abstract="true">
        <association xsi:type="FM:OptionalAssociation" childfeature="LimitBranches" parentfeature="MultiBranch"/>
        <subfeatures name="LimitBranches"/>
      </subfeatures>
    </subfeatures>
  </feature>
  <feature name="CollaborationStyle">
    <association xsi:type="FM:MandatoryAssociation" childfeature="CollaborationStyleType" parentfeature="CollaborationStyle"/>
    <subfeatures name="CollaborationStyleType" abstract="true">
      <association xsi:type="FM:OptionalAssociation" childfeature="LocalOnly" parentfeature="CollaborationStyleType"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="Remote" parentfeature="CollaborationStyleType"/>
      <subfeatures name="LocalOnly"/>
      <subfeatures name="Remote"/>
    </subfeatures>
  </feature>
  <feature name="Database">
    <association xsi:type="FM:MandatoryAssociation" childfeature="DataBaseType" parentfeature="Database"/>
    <subfeatures name="DataBaseType" abstract="true">
      <association xsi:type="FM:OptionalAssociation" childfeature="Neo4J" parentfeature="DataBaseType"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="XMI" parentfeature="DataBaseType"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="SQL" parentfeature="DataBaseType"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="MongoDB" parentfeature="DataBaseType"/>
      <subfeatures name="Neo4J"/>
      <subfeatures name="XMI"/>
      <subfeatures name="SQL"/>
      <subfeatures name="MongoDB"/>
    </subfeatures>
  </feature>
  <feature name="VersionUnit">
    <association xsi:type="FM:MandatoryAssociation" childfeature="VersionUnitType" parentfeature="VersionUnit"/>
    <subfeatures name="VersionUnitType" abstract="true">
      <association xsi:type="FM:OptionalAssociation" childfeature="File" parentfeature="VersionUnitType"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="Semantic" parentfeature="VersionUnitType"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="Element" parentfeature="VersionUnitType"/>
      <association xsi:type="FM:OptionalAssociation" childfeature="Line" parentfeature="VersionUnitType"/>
      <subfeatures name="File"/>
      <subfeatures name="Semantic"/>
      <subfeatures name="Element"/>
      <subfeatures name="Line"/>
    </subfeatures>
  </feature>
</FM:RootFeature>
