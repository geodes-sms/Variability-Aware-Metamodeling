<?xml version="1.0" encoding="UTF-8"?>
<FM:RootFeature
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:FM="http://www.example.org/FM"
    name="ReLiS"
    abstract="true">
  <association
      xsi:type="FM:OptionalAssociation"
      childfeature="Synthesis"
      parentfeature="ReLiS"/>
  <association
      xsi:type="FM:OptionalAssociation"
      childfeature="Screening"
      parentfeature="ReLiS"/>
  <feature name="Synthesis"
      abstract="true">
    <association
        xsi:type="FM:MandatoryAssociation"
        childfeature="Type"
        parentfeature="Synthesis"/>
    <association
        xsi:type="FM:OptionalAssociation"
        childfeature="CompareGraph"
        parentfeature="Synthesis"/>
    <subfeatures
        name="Type"
        abstract="true">
      <association
          xsi:type="FM:MandatoryAssociation"
          childfeature="Bar"
          parentfeature="Type"/>
      <subfeatures
          name="Bar"/>
    </subfeatures>
    <subfeatures
        name="CompareGraph"/>
  </feature>
  <feature name="Screening"
      abstract="true">
    <association
        xsi:type="FM:MandatoryAssociation"
        childfeature="Phase"
        parentfeature="Screening"/>
    <subfeatures
        name="Phase">
      <association
          xsi:type="FM:MandatoryAssociation"
          childfeature="Field"
          parentfeature="Phase"/>
      <subfeatures
          name="Field"/>
    </subfeatures>
  </feature>
</FM:RootFeature>
